{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport md5 from \"js-md5\";\nexport default {\n  name: \"Login\",\n  data: function data() {\n    return {\n      user: {}\n    };\n  },\n  methods: {\n    onSubmit: function onSubmit() {\n      var _this = this;\n\n      if (this.user.username === '' || this.user.password === '' || this.user.confirmPassword === '') {\n        this.$message.error(\"账号或密码不能为空\");\n        return false;\n      }\n\n      if (this.user.password !== this.user.confirmPassword) {\n        this.$message.error(\"两次密码不一致\");\n        return false;\n      }\n\n      this.user.password = md5(this.user.password);\n      this.request.post(\"/register\", this.user).then(function (res) {\n        if (res.code === '200') {\n          _this.$message.success(\"注册成功\");\n\n          _this.$router.push(\"/login\");\n        } else {\n          _this.$message.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmCA;AAEA;AACAA,eADA;AAEAC,MAFA,kBAEA;AACA;AACAC;AADA;AAGA,GANA;AAOAC;AACAC,YADA,sBACA;AAAA;;AACA;AACA;AACA;AACA;;AACA;AACA;AACA;AACA;;AACA;AACA;AACA;AACA;;AACA;AACA,SAHA,MAGA;AACA;AACA;AACA,OAPA;AAQA;AAnBA;AAPA","names":["name","data","user","methods","onSubmit"],"sourceRoot":"src/views","sources":["Register.vue"],"sourcesContent":["<!--\n * 用户注册页面\n *\n * @author: ZhangDaYe\n * @Date: 2023-11-11\n-->\n<template>\n<div class=\"wrapper\" >\n <div class=\"login-box\">\n\n      <div class=\"title\">\n        <b><span style=\"color: #e75c09\">注 册</span></b>\n      </div>\n      <div style=\"margin-top: 30px\">\n        <el-form label-width=\"70px\">\n          <el-form-item label=\"用户名\">\n            <el-input v-model.trim=\"user.username\" aria-required=\"true\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"密码\" style=\"margin-top: 25px\">\n            <el-input v-model.trim=\"user.password\"show-password aria-required=\"true\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"确认密码\" style=\"margin-top: 25px\">\n            <el-input v-model.trim=\"user.confirmPassword\" show-password aria-required=\"true\"></el-input>\n          </el-form-item>\n          <el-form-item style=\"margin: 30px 80px\">\n            <el-button type=\"success\" @click=\"onSubmit\">注册</el-button>\n            <el-button @click=\"$router.push('/login')\">返回</el-button>\n          </el-form-item>\n        </el-form>\n      </div>\n </div>\n</div>\n</template>\n\n<script>\nimport md5 from \"js-md5\";\n\nexport default {\n  name: \"Login\",\n  data(){\n    return {\n      user: {},\n    }\n  },\n  methods: {\n    onSubmit() {\n      if(this.user.username==='' || this.user.password==='' ||this.user.confirmPassword===''){\n        this.$message.error(\"账号或密码不能为空\")\n        return false;\n      }\n      if(this.user.password!==this.user.confirmPassword){\n        this.$message.error(\"两次密码不一致\")\n        return false;\n      }\n      this.user.password = md5(this.user.password);\n      this.request.post(\"/register\",this.user).then(res=>{\n        if(res.code==='200'){\n          this.$message.success(\"注册成功\")\n          this.$router.push(\"/login\")\n        }else{\n          this.$message.error(res.msg)\n        }\n      })\n    }\n  }\n}\n</script>\n\n<style scoped>\n.wrapper {\n  height: 100vh;\n  background-image: url(\"@/resource/img/login_back.png\");\n  overflow: auto;\n}\n.login-box {\n  margin: 220px auto;\n  padding: 40px;\n  width: 450px;\n  height: 310px;\n  background-color: #ffffff;\n  border-radius: 10px;\n}\n.title {\n  text-align: center;\n  margin-top: 10px;\n  font-size: 25px;\n  color: #000000\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}