 load() {\n      var _this2 = this;\n\n      API.get(url + \"fullPage\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          searchText: this.searchText\n        }\n      }).then(function (res) {\n        _this2.tableData = res.data.records;\n        _this2.total = res.data.total;\n      });\n    },\n    //重置搜索\n    reset: function reset() {\n      this.searchText = '';\n      this.load();\n    },\n    //新增\n    add: function add() {\n      this.$router.push(\"goodInfo\");\n    },\n    //编辑\n    edit: function edit(obj) {\n      this.entity = JSON.parse(JSON.stringify(obj));\n      this.$router.push({\n        name: 'goodInfo',\n        query: {\n          good: JSON.stringify(this.entity)\n        }\n      });\n    },\n    //图片上传成功\n    handleImgSuccess: function handleImgSuccess(res) {\n      var _this3 = this;\n\n      this.entity.imgs = res.data;\n      API.post(url, this.entity).then(function (res2) {\n        if (res2.code === '200') {\n          _this3.$message({\n            type: \"success\",\n            message: \"操作成功\"\n          });\n        } else {\n          _this3.$message({\n            type: \"error\",\n            message: res2.msg\n          });\n        }\n\n        _this3.load();\n\n        _this3.dialogFormVisible = false;\n      });\n    },\n    //保存商品\n    save: function save() {\n      var _this4 = this;\n\n      //上传图片\n      if (this.fileList.length !== 0) {\n        this.$refs.upload.submit();\n      } else {\n        //不上传图片\n        API.post(url, this.entity).then(function (res2) {\n          if (res2.code === '200') {\n            _this4.$message({\n              type: \"success\",\n              message: \"操作成功\"\n            });\n          } else {\n            _this4.$message({\n              type: \"error\",\n              message: res2.msg\n            });\n          }\n\n          _this4.load();\n\n          _this4.dialogFormVisible = false;\n        });\n      }\n    },\n    //删除商品\n    del: function del(id) {\n      var _this5 = this;\n\n      API.delete(url + id).then(function (res) {\n        _this5.$message({\n          type: \"success\",\n          message: \"操作成功\"\n        });\n\n        _this5.load();\n      });\n    },\n    handleChange: function handleChange(file, fileList) {\n      this.fileList = fileList.slice(-3);\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuIA;AACA;AAEA;AACAA,eADA;AAEA;AACAC,MAHA,kBAGA;AACA;AACAC,wCADA;AAEAC,kBAFA;AAGAC,iBAHA;AAIAC,oBAJA;AAKAC,cALA;AAMAC,mBANA;AAOAC,gBAPA;AAQAC,iBARA;AASAC,gBATA;AAUAC,cAVA;AAWAC;AAXA;AAaA,GAjBA;AAkBA;AACAC,SAnBA,qBAmBA;AACA;AACA;AAEA,GAvBA;AAwBA;AACAC;AACA;AACAC,oBAFA,4BAEAN,QAFA,EAEA;AACA;AACA;AACA,KALA;AAMA;AACAO,uBAPA,+BAOAR,OAPA,EAOA;AACA;AACA;AACA,KAVA;AAWA;AACAS,mBAZA,2BAYAC,IAZA,EAYA;AAAA;;AACAC;AACAC;AACAC,qBADA;AAEAC;AAFA;AADA,SAKAC,IALA,CAKA;AACA;AACA;AACA,SAFA,MAEA;AACA;AACA;AACA,OAXA;AAYA,KAzBA;AA0BA;AACAC,QA3BA,kBA2BA;AAAA;;AACAL;AACAC;AACAZ,+BADA;AAEAC,iCAFA;AAGAJ;AAHA;AADA,SAMAkB,IANA,CAMA;AACA;AACA;AACA,OATA;AAUA,KAtCA;AAuCA;AACAE,SAxCA,mBAwCA;AACA;AACA;AACA,KA3CA;AA4CA;AACAC,OA7CA,iBA6CA;AACA;AACA,KA/CA;AAgDA;AACAC,QAjDA,gBAiDAC,GAjDA,EAiDA;AACA;AACA;AAAA5B;AAAA6B;AAAAX;AAAA;AAAA;AACA,KApDA;AAqDA;AACAY,oBAtDA,4BAsDAC,GAtDA,EAsDA;AAAA;;AACA;AACAZ;AACA;AACA;AACAa,2BADA;AAEAC;AAFA;AAIA,SALA,MAKA;AACA;AACAD,yBADA;AAEAC;AAFA;AAIA;;AACA;;AACA;AACA,OAdA;AAeA,KAvEA;AAwEA;AACAC,QAzEA,kBAyEA;AAAA;;AACA;AACA;AACA;AACA,OAFA,MAEA;AACA;AACAf;AACA;AACA;AACAa,6BADA;AAEAC;AAFA;AAIA,WALA,MAKA;AACA;AACAD,2BADA;AAEAC;AAFA;AAIA;;AACA;;AACA;AACA,SAdA;AAeA;AACA,KA/FA;AAgGA;AACAE,OAjGA,eAiGAd,EAjGA,EAiGA;AAAA;;AACAF;AACA;AACAa,yBADA;AAEAC;AAFA;;AAIA;AACA,OANA;AAOA,KAzGA;AA0GAG,gBA1GA,wBA0GAC,IA1GA,EA0GAlC,QA1GA,EA0GA;AACA;AACA;AA5GA;AAzBA","names":["name","data","baseApi","fileList","options","searchText","user","tableData","pageNum","pageSize","entity","total","dialogFormVisible","created","methods","handleSizeChange","handleCurrentChange","handleRecommend","good","API","params","id","isRecommend","then","load","reset","add","edit","obj","query","handleImgSuccess","res","type","message","save","del","handleChange","file"],"sourceRoot":"src/views/manage/good","sources":["Goods.vue"],"sourcesContent":["<!--\n * 商品管理\n *\n<<<<<<< HEAD\n * @author: ZhangDaYe\n=======\n * @Author: ShanZhu\n>>>>>>> origin/feature_2024_08_24\n * @Date: 2023-11-11\n-->\n<template>\n  <div>\n    <div style=\"padding: 5px 0\">\n      <el-input v-model=\"searchText\" @keyup.enter.native=\"load\" style=\"width: 200px\">\n        <i slot=\"prefix\" class=\"el-input__icon el-icon-search\"></i></el-input>\n      <el-button @click=\"load\" round type=\"primary\" style=\"margin: 5px\">搜索</el-button>\n      <el-button @click=\"reset\" round type=\"info\" style=\"margin: 5px\">重置</el-button>\n      <el-button @click=\"add\" round type=\"success\" style=\"margin: 5px\">新增</el-button>\n    </div>\n\n    <el-table :data=\"tableData\" stripe border fixed style=\"width: 100%\">\n      <el-table-column prop=\"id\" label=\"商品id\" width=\"80px\"></el-table-column>\n\n      <el-table-column prop=\"name\" label=\"商品名称\"></el-table-column>\n\n      <el-table-column label=\"商品图片\" width=\"120px\">\n        <template slot-scope=\"scope\">\n          <img :src=\"baseApi + scope.row.imgs\" style=\"width: 90px;height: 80px\">\n        </template>\n      </el-table-column>\n\n      <el-table-column prop=\"description\" label=\"商品描述\"></el-table-column>\n\n      <el-table-column prop=\"discount\" label=\"折扣\"></el-table-column>\n\n      <el-table-column prop=\"sales\" label=\"销量\"></el-table-column>\n\n      <el-table-column prop=\"saleMoney\" label=\"销售额（元)\"></el-table-column>\n\n      <el-table-column prop=\"createTime\" label=\"创建时间\">\n        <template slot-scope=\"scope\">\n          {{scope.row.createTime.replace(\" \",\"&nbsp;&nbsp;\")}}\n        </template>\n\n      </el-table-column>\n\n      <el-table-column label=\"推荐\" width=\"150\" >\n        <template slot-scope=\"scope\">\n          <el-switch\n              v-model=\"scope.row.recommend\"\n              @change=\"handleRecommend(scope.row)\"\n              active-color=\"#13ce66\"\n              inactive-color=\"#ff4949\">\n          </el-switch>\n        </template>\n      </el-table-column>\n\n      <el-table-column\n          fixed=\"right\"\n          label=\"操作\"\n          width=\"200\">\n        <template slot-scope=\"scope\">\n          <el-button type=\"primary\" icon=\"el-icon-edit\" circle  @click=\"edit(scope.row)\"></el-button>\n          <el-popconfirm\n              @confirm=\"del(scope.row.id)\"\n              title=\"确定删除？\"\n          >\n            <el-button type=\"danger\" icon=\"el-icon-delete\" circle slot=\"reference\" style=\"margin-left: 10px\"></el-button>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <!--  分页控件  -->\n    <div style=\"margin-top: 10px\">\n      <el-pagination\n        @size-change=\"handleSizeChange\"\n        @current-change=\"handleCurrentChange\"\n        :current-page=\"pageNum\"\n        :page-size=\"pageSize\"\n        :page-sizes=\"[3, 5, 8, 10]\"\n        layout=\"total, sizes, prev, pager, next, jumper\"\n        :total=\"total\"\n      >\n      </el-pagination>\n    </div>\n\n    <!--新增商品弹窗-->\n    <el-dialog title=\"信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\"\n               :close-on-click-modal=\"false\">\n      <el-form :model=\"entity\">\n\n        <el-form-item label=\"商品名称\" label-width=\"150px\">\n          <el-input v-model=\"entity.name\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n\n        <el-form-item label=\"商品描述\" label-width=\"150px\">\n          <el-input v-model=\"entity.description\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n\n        <el-form-item label=\"折扣\" label-width=\"150px\">\n          <el-input v-model=\"entity.discount\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n\n        <el-form-item label=\"分类id\" label-width=\"150px\">\n          <el-input v-model=\"entity.categoryId\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n\n        <el-form-item label=\"商品图片\" label-width=\"150px\">\n          <el-upload\n              class=\"upload-demo\"\n              ref=\"upload\"\n              :action=\"baseApi + '/file/upload'\"\n              :file-list=\"fileList\"\n              :on-change=\"handleChange\"\n              :limit=\"2\"\n              :on-success=\"handleImgSuccess\"\n              :auto-upload=\"false\">\n            <el-button slot=\"trigger\" size=\"small\" type=\"primary\">选取文件</el-button>\n            <div slot=\"tip\" class=\"el-upload__tip\">只能上传jpg/png文件，且不超过500kb</div>\n          </el-upload>\n        </el-form-item>\n\n      </el-form>\n\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n      </div>\n\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport API from '../../../utils/request'\nconst url = \"/api/good/\"\n\nexport default {\n  name: \"Goods\",\n  //页面初始化数据\n  data() {\n    return {\n      baseApi: this.$store.state.baseApi,\n      fileList: [],\n      options: [],\n      searchText: '',\n      user: {},\n      tableData: [],\n      pageNum: 1,\n      pageSize: 5,\n      entity: {},\n      total: 0,\n      dialogFormVisible: false\n    };\n  },\n  //页面加载完成\n  created() {\n    this.user = localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {}\n    this.load()\n\n  },\n  //方法\n  methods: {\n    //页面变化\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize\n      this.load()\n    },\n    //翻页\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum\n      this.load()\n    },\n    //修改商品推荐\n    handleRecommend(good){\n      API.get(url + \"recommend\", {\n        params: {\n          id: good.id,\n          isRecommend : good.recommend,\n        }\n      }).then(res => {\n        if(res.code==='200'){\n          this.$message.success(\"修改成功\")\n        }else{\n          this.$message.error(res.msg)\n        }\n      })\n    },\n    //页面加载\n    load() {\n       API.get(url + \"fullPage\", {\n          params: {\n            pageNum: this.pageNum,\n            pageSize: this.pageSize,\n            searchText: this.searchText\n          }\n       }).then(res => {\n          this.tableData = res.data.records\n          this.total = res.data.total\n       })\n    },\n    //重置搜索\n    reset(){\n      this.searchText = '';\n      this.load()\n    },\n    //新增\n    add() {\n      this.$router.push(\"goodInfo\")\n    },\n    //编辑\n    edit(obj) {\n      this.entity = JSON.parse(JSON.stringify(obj))\n      this.$router.push({name: 'goodInfo',query:{good: JSON.stringify(this.entity)}})\n    },\n    //图片上传成功\n    handleImgSuccess(res){\n      this.entity.imgs = res.data;\n      API.post(url, this.entity).then(res2 => {\n        if (res2.code === '200') {\n          this.$message({\n            type: \"success\",\n            message: \"操作成功\"\n          })\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res2.msg\n          })\n        }\n        this.load()\n        this.dialogFormVisible = false\n      })\n    },\n    //保存商品\n    save() {\n      //上传图片\n      if(this.fileList.length!==0){\n        this.$refs.upload.submit();\n      }else{\n        //不上传图片\n        API.post(url, this.entity).then(res2 => {\n          if (res2.code === '200') {\n            this.$message({\n              type: \"success\",\n              message: \"操作成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res2.msg\n            })\n          }\n          this.load()\n          this.dialogFormVisible = false\n        })\n      }\n    },\n    //删除商品\n    del(id) {\n      API.delete(url + id).then(res => {\n        this.$message({\n          type: \"success\",\n          message: \"操作成功\"\n        })\n        this.load()\n      })\n    },\n    handleChange(file, fileList){\n      this.fileList = fileList.slice(-3);\n    },\n\n  }\n\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}