mb style=\"display: inline-block; margin-left: 30px;font-size: 17px\">\n      <el-breadcrumb-item :to=\"{ path: '/manage/home' }\">首页</el-breadcrumb-item>\n      <el-breadcrumb-item>{{routePath}}</el-breadcrumb-item>\n    </el-breadcrumb>\n    </div>\n\n    <!--设置按钮-->\n    <el-dropdown style=\"margin-right: 40px;cursor: pointer\">\n    <span class=\"el-dropdown-link\">\n      <div style=\"display: inline-block\">\n        <img :src=\"baseApi + user.avatarUrl\" class=\"avatar\">\n             {{user.nickname }}\n      <i class=\"el-icon-arrow-down el-icon--right\" style=\"margin-right: 15px\"></i>\n      </div>\n    </span>\n\n      <el-dropdown-menu slot=\"dropdown\" style=\"text-align: center\">\n        <el-dropdown-item>\n          <div @click=\"$router.push('/manage/person')\">个人信息</div>\n        </el-dropdown-item>\n        <el-dropdown-item>\n          <div @click=\"logout\">退出</div>\n        </el-dropdown-item>\n      </el-dropdown-menu>\n    </el-dropdown>\n\n  </div>\n\n</template>\n\n<script>\n\nexport default {\n  name: \"Header\",\n  props: {\n    collapseIcon: String,\n    collapseTitle: String,\n    user: Object\n  },\n  methods:{\n    //登出\n    logout() {\n      localStorage.removeItem(\"user\");\n      this.$router.push('/login');\n      this.$message.success(\"退出成功\");\n    },\n    back(){\n      this.$router.go(-1)\n    }\n\n  },\n  data(){\n    return{\n      routePath: '',\n      baseApi: this.$store.state.baseApi,\n    }\n  },\n  watch: {\n    //面包屑\n    '$route': function (){\n      this.routePath=this.$route.meta.path\n    },\n\n  },\n  created() {\n    this.routePath=this.$route.meta.path;\n  }\n}\n\n</script>\n\n<style scoped>\n.avatar{\n  width: 30px;\n  border-radius: 50%;\n  position: relative;\n  top: 10px;\n  right: 5px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}